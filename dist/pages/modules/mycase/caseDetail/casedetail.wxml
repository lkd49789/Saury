            </view>
            <view class="personal-item2" bindtap="toclientlinkman" wx:if="{{caseDetailData.clientData.linkerPhoto.length!==0}}">
              <view class="personal-item2-two">
                <text style="color:#7a7a7a">客户联系人
                                                                                                          </text>
                <icon class="iconfont icon-gengduo" style="font-size:30rpx;color:#b2b2b2"/>
              </view>
