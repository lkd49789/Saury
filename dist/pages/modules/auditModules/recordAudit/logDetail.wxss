.body {
  height: 100%;
  position: relative;
}
.body .container-title {
  height: 104rpx;
  font-size: 48rpx;
  color: #2a2a2a;
  line-height: 104rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #dedede;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background: #fff;
  z-index: 9999;
}
.body .container-title > text {
  margin-left: 28rpx;
}
.body .container-title icon {
  margin-right: 28rpx;
}
.body .logDetail-container {
  font-size: 28rpx;
  color: #2a2a2a;
}
.body .logDetail-container .bold::after {
  content: '';
  display: block;
  height: 14rpx;
  background: #f4f4f4;
}
.body .logDetail-container .bold-line {
  height: 7px;
  background: #f4f4f4;
}
