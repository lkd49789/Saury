
    <view class="container">
        <view class="fixed-view">
            <view class="case-top">
                <text class="title">文书审核</text>
                <icon class="iconfont icon-sousuo1 " style="font-size:60rpx;color:#5d73fa" bindtap="toSearch"/>
            </view>
            
    <view class="navbar">
        <scroll-view scroll-x>
            <block wx:for="{{$navbar$navbars}}" wx:for-index="$navbar$index" wx:for-item="$navbar$item" wx:key="$navbar$index">
                <view class="item {{$navbar$currentTab == $navbar$index ? 'active' : ''}} " bindtap="$navbar$navbarTap" data-wpynavbartap-a="{{$navbar$index}}">
                    <text>{{$navbar$item}}</text>
                    <view  wx:if="{{$navbar$currentTab == $navbar$index}}" class="navbar_box"/>
                </view>
            </block>
        </scroll-view>
    </view>

        </view>
        <view class="main"  wx:if="{{currentTab==0}}">
            <block wx:for="{{GetApproveCaseFileListData_W}}" wx:for-index="index" wx:for-item="item" wx:key="index">
                <view bindtap="toApproveDetailAudit" data-wpytoapprovedetailaudit-a="{{item.id}}">
                    <view class="caseName oneLine-hidden">
                        <text class="omit">{{item.caseName}}</text>
                    </view>
                    <view class="title_stampNumber">
                        <text class="omit">{{item.title}}</text>
                        <text class="omit">{{item.stampNumber}}</text>
                    </view>
                    <view class="documentTypeText_stampTypeText">
                        <text class="omit">{{item.documentTypeText}}</text>
                        <text class="omit">{{item.stampTypeText}}</text>
                    </view>
                </view>
            </block>
        </view>
        <view class="main"  wx:if="{{currentTab==1}}">
            <block wx:for="{{GetApproveCaseFileListData_A}}" wx:for-index="index" wx:for-item="item" wx:key="index">
                <view bindtap="toApproveDetailAudit" data-wpytoapprovedetailaudit-a="{{item.id}}">
                    <view class="caseName  oneLine-hidden">
                        <text class="omit">{{item.caseName}}</text>
                    </view>
                    <view class="title_stampNumber">
                        <text class="omit">{{item.title}}</text>
                        <text class="omit">{{item.stampNumber}}</text>
                    </view>
                    <view class="documentTypeText_stampTypeText">
                        <text class="omit">{{item.documentTypeText}}</text>
                        <text class="omit">{{item.stampTypeText}}</text>
                    </view>
                </view>
            </block>
        </view>
        <view class="main" wx:if="{{currentTab==2}}">
            <block wx:for="{{GetApproveCaseFileListData_R}}" wx:for-index="index" wx:for-item="item" wx:key="index">
                <view bindtap="toApproveDetailAudit" data-wpytoapprovedetailaudit-a="{{item.id}}">
                    <view class="caseName  oneLine-hidden">
                        <text class="omit">{{item.caseName}}</text>
                    </view>
                    <view class="title_stampNumber">
                        <text class="omit">{{item.title}}</text>
                        <text class="omit">{{item.stampNumber}}</text>
                    </view>
                    <view class="documentTypeText_stampTypeText">
                        <text class="omit">{{item.documentTypeText}}</text>
                        <text class="omit">{{item.stampTypeText}}</text>
                    </view>
                </view>
            </block>
        </view>
       
    <view  wx:if="{{$placeHolderImage$placeHolder.placeHolderShow}}" class="placeHolderImage" catchtouchmove="$placeHolderImage$true">
                <image src="{{$placeHolderImage$placeHolderImage[$placeHolderImage$placeHolder.placeHolderImageIndex]}}" mode="scaleToFill" lazy-load="false" style="width:580rpx;height:580rpx">
                </image>
    </view>

    </view>