@import "./../../../../../components/navbar.wxss";
@import "./../../../../../components/placeHolderImage.wxss";
.body {
  height: 100%;
}
.body .fixed-view {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background: #fff;
  z-index: 999;
}
.body .fixed-view .case-top {
  height: 104rpx;
  line-height: 104rpx;
  padding: 0 28rpx;
  font-size: 48rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.body .main {
  position: relative;
  margin-top: 230rpx;
}
.body .main .case .one-chunk {
  padding: 0 28rpx;
}
.body .main .case .one-chunk > view:nth-child(1) {
  color: #5d73fa;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-bottom: 36rpx;
}
.body .main .case .one-chunk > view:nth-child(2) {
  margin-bottom: 20rpx;
}
.body .main .case .one-chunk > view:nth-child(3) {
  margin-bottom: 20rpx;
}
.body .main .case .one-chunk > view:nth-child(4) {
  margin-bottom: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.body .main .case .one-chunk > view:nth-child(5) {
  margin-bottom: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.body .main .case .one-chunk > view:nth-child(6) {
  color: #ff9900;
  margin-bottom: 36rpx;
}
.body .main .case .two-chunk {
  padding: 30rpx 28rpx 0 28rpx;
}
.body .main .case .two-chunk > view:nth-child(1) {
  display: flex;
  align-items: center;
  margin-bottom: 30rpx;
}
.body .main .case .two-chunk > view:nth-child(1) > view {
  display: flex;
  flex-direction: column;
  width: 50%;
}
.body .main .case .two-chunk > view:nth-child(1) > view > text:first-child {
  margin-bottom: 14rpx;
  color: #7a7a7a;
}
.body .main .case .two-chunk > view:not(:nth-child(1)) {
  display: flex;
  flex-direction: column;
  width: 100%;
  margin-bottom: 30rpx;
}
.body .main .case .two-chunk > view:not(:nth-child(1)) > text:first-child {
  margin-bottom: 14rpx;
  color: #7a7a7a;
}
.body .main .case .three-chunk {
  padding: 30rpx 28rpx;
}
.body .main .case .three-chunk > view:not(:last-child) {
  margin-bottom: 36rpx;
  display: flex;
  flex-direction: column;
}
.body .main .case .three-chunk > view:not(:last-child) > text:first-child {
  color: #7a7a7a;
  margin-bottom: 20rpx;
}
.body .main .case .three-chunk > view:last-child > view:first-child {
  margin-bottom: 20rpx;
}
.body .main .case .three-chunk > view:last-child > view:last-child {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.body .main .case .three-chunk > view:last-child > view:last-child > view {
  margin: 0 20rpx 30rpx 0;
  padding: 8rpx 18rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid #5d73fa;
  border-radius: 20rpx;
}
.body .main .case .four-chunk {
  padding: 30rpx 28rpx;
}
.body .main .case .four-chunk > view:first-child {
  margin-bottom: 32rpx;
  color: #7a7a7a;
}
.body .main .client {
  margin-bottom: 60rpx;
}
.body .main .client .one-chunk {
  padding: 0 28rpx 30rpx 28rpx;
}
.body .main .client .one-chunk > view:first-child {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 30rpx;
}
.body .main .client .one-chunk > view:nth-child(2) {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}
.body .main .client .one-chunk > view:nth-child(2) > text:last-child {
  color: #7a7a7a;
  font-weight: bold;
}
.body .main .client .one-chunk > view:nth-child(3) {
  color: #7a7a7a;
  margin-bottom: 20rpx;
}
.body .main .client .one-chunk > view:nth-child(4) {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.body .main .client .one-chunk > view:nth-child(4) > view {
  margin-bottom: 20rpx;
  padding: 5rpx 15rpx;
  border-radius: 20rpx;
  border: 1px solid #5d73fa;
  margin-right: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #5d73fa;
}
.body .main .client .one-chunk > view:nth-child(5) {
  margin-bottom: 20rpx;
}
.body .main .client .one-chunk > view:last-child .icon {
  font-size: 40rpx;
  color: #ff9900;
  margin-right: 20rpx;
}
.body .main .client .two-chunk > view {
  padding: 30rpx 28rpx;
}
.body .main .client .two-chunk > view > view:first-child {
  margin-bottom: 32rpx;
}
.body .main .client .two-chunk > view > view:not(:first-child):not(:last-child) {
  margin-bottom: 20rpx;
}
.body .main .client .three-chunk {
  padding: 30rpx 28rpx 0 28rpx;
}
.body .main .client .three-chunk > view:first-child {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 32rpx;
}
.body .main .client .three-chunk > view:last-child {
  position: relative;
  display: flex;
  align-items: center;
  height: 60rpx;
  width: 100%;
}
.body .main .client .three-chunk > view:last-child > image {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  position: absolute;
}
.body .main .lawyer > view:last-child {
  background: #fff;
}
.body .main .lawyer > view:nth-child(2n+1) {
  padding: 30rpx 28rpx;
}
.body .main .lawyer > view:nth-child(2n+1) > view:first-child {
  display: flex;
  align-items: center;
  margin-bottom: 32rpx;
  color: #7a7a7a;
}
.body .main .lawyer > view:nth-child(2n+1) > view:last-child {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.body .main .lawyer > view:nth-child(2n+1) > view:last-child > view {
  display: flex;
  align-items: center;
  flex-direction: column;
  margin: 0 28rpx 20rpx 0;
}
.body .main .lawyer > view:first-child {
  padding: 0rpx 28rpx 30rpx 28rpx;
}
.body .main .caseContractList {
  margin: 0 28rpx;
}
.body .main .caseContractList > view {
  display: flex;
  align-items: center;
  padding: 30rpx 20rpx;
  margin-bottom: 44rpx;
  box-shadow: 0px 8px 30px rgba(65, 98, 213, 0.25);
  border-radius: 20rpx;
}
.body .main .caseContractList > view > view:first-child {
  width: 20%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.body .main .caseContractList > view > view:last-child {
  display: flex;
  flex-direction: column;
}
.body .main .caseContractList > view > view:last-child > text:first-child {
  margin-bottom: 20rpx;
}
.body .main .orderItem > view:nth-child(1) {
  padding: 0rpx 28rpx 30rpx 28rpx;
}
.body .main .orderItem > view:nth-child(1) > view:first-child {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.body .main .orderItem > view:nth-child(1) > view:nth-child(2) {
  margin-bottom: 20rpx;
}
.body .main .orderItem > view:nth-child(1) > view:last-child {
  display: flex;
  flex-direction: column;
}
.body .main .orderItem > view:nth-child(1) > view:last-child > text:first-child {
  margin-bottom: 14rpx;
  color: #7a7a7a;
}
.body .main .orderItem > view:nth-child(3) {
  padding: 30rpx 28rpx;
}
.body .main .orderItem > view:nth-child(3) .common {
  display: flex;
  flex-direction: column;
  margin-bottom: 20rpx;
}
.body .main .orderItem > view:nth-child(3) .common > text:first-child {
  margin-bottom: 14rpx;
  color: #7a7a7a;
}
.body .main .orderItem > view:nth-child(3) .keyword {
  display: flex;
  flex-direction: column;
}
.body .main .orderItem > view:nth-child(3) .keyword > view:first-child {
  margin-bottom: 14rpx;
  color: #7a7a7a;
}
.body .main .orderItem > view:nth-child(3) .keyword > view:last-child {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  color: #5d73fa;
}
.body .main .orderItem > view:nth-child(3) .keyword > view:last-child > view {
  padding: 5rpx 15rpx;
  border-radius: 20rpx;
  border: 1px solid #5d73fa;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.body .main .orderItem > view:nth-child(3) > view:last-child {
  margin-bottom: 0;
}
.body .main .orderItem > view:nth-child(5) > view {
  padding: 30rpx 28rpx;
  border-bottom: 1px solid #dfdfdf;
}
.body .main .orderItem > view:nth-child(5) > view > view {
  display: flex;
  flex-direction: column;
  margin-bottom: 20rpx;
}
.body .main .orderItem > view:nth-child(5) > view > view > text:first-child {
  color: #7a7a7a;
  margin-bottom: 14rpx;
}
.body .main .orderItem > view:nth-child(5) > view > view:first-child {
  display: flex;
  flex-direction: column;
  margin-bottom: 20rpx;
}
.body .main .orderItem > view:nth-child(5) > view > view:first-child > text:first-child {
  color: #7a7a7a;
  margin-bottom: 14rpx;
}
.body .main .orderItem > view:nth-child(5) > view > view:first-child > text:nth-child(2) {
  margin-bottom: 14rpx;
}
.body .main .orderItem > view:nth-child(5) > view:first-child {
  border: 0;
}
.body .auditBth {
  position: fixed;
  bottom: 0;
  width: 100%;
  background: #5d73fa;
  color: #fff;
  height: 98rpx;
  font-size: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 999;
}
.body .interval-chunk {
  width: 100%;
  height: 15rpx;
  background: #f4f4f4;
}
.body .noDataImage {
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
