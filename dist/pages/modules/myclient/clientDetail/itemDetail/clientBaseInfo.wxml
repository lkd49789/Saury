
    <view class="body">
        <view class="top-title">
            <text>客户基本信息</text>
        </view>
        <view class="main">
            <view>
                <view>
                    <view>
                        <text>{{clientBaseData.name||'未填写'}}</text>
                    </view>
                    <view>
                        <view>
                            <text>{{clientBaseData.enName||'未填写'}}</text>
                            <text style="font-weight: bold;color:#7a7a7a">{{clientBaseData.shortName||'未填写'}}</text>
                        </view>
                    </view>
                    <view>
                        <text style="color:#7a7a7a">{{clientBaseData.id||'未填写'}}</text>
                    </view>
                    <view>
                        <view>
                            <text>{{clientBaseData.clientTypeText||'未填写'}}</text>
                        </view>
                        <view>
                            <text>{{clientBaseData.importLevelText||'未填写'}}</text>
                        </view>
                    </view>
                    <view>
                        <icon class="iconfont icon-dizhi" style="color:#5d73fa;font-size:30rpx;margin-right:20rpx"/>
                        <text>{{clientBaseData.regionText||'未填写'}}</text>
                    </view>
                    <view>
                        <text style="color:#7a7a7a">业务类别</text>
                        <text>{{clientBaseData.categoryText||'未填写'}}</text>
                    </view>
                </view>
            </view>
            <view  wx:if="{{clientBaseData.categoryText!=='公民个人'}}">
                <view>
                    <view style="margin-bottom: 36rpx">
                        <text style="font-size:30rpx;color:#2a2a2a">客户负责人/法定代表人信息</text>
                    </view>
                    <view>
                        <text style="padding-right:10rpx;border-right:1px solid #7a7a7a;margin-right:10rpx">{{clientBaseData.legalDutyText||'未填写'}}</text>
                        <text style="color:#5d73fa">{{clientBaseData.legalPersonText||'未填写'}}</text>
                    </view>
                    <view>
                        <icon class="iconfont icon-dizhi" style="font-size:40rpx;margin-right:20rpx;color:#ff9900"/>
                        <text>{{clientBaseData.linker||'未填写'}}</text>
                    </view>
                    <view>
                        <icon class="iconfont icon-dianhua" style="font-size:40rpx;margin-right:20rpx;color:#ff9900"/>
                        <text>{{clientBaseData.landline||'未填写'}}</text>
                    </view>
                    <view>
                        <icon class="iconfont icon-youxiang" style="font-size:40rpx;margin-right:20rpx;color:#ff9900"/>
                        <text>{{clientBaseData.email||'未填写'}}</text>
                    </view>
                    <view>
                        <icon class="iconfont icon-chuanzhen" style="font-size:40rpx;margin-right:20rpx;color:#ff9900"/>
                        <text>{{clientBaseData.fax||'未填写'}}</text>
                    </view>
                </view>
            </view>
            <view  wx:else>
                <view>
                    <view>
                        <text style="font-size:30rpx;color:#2a2a2a">自然人</text>
                    </view>
                    <view>
                        <icon class="iconfont icon-shenfenzheng" style="font-size:40rpx;margin-right:20rpx;color:#ff9900"/>
                        <text>{{clientBaseData.cardNo||'未填写'}}</text>
                    </view>
                    <view>
                        <icon class="iconfont icon-minzu" style="font-size:40rpx;margin-right:20rpx;color:#ff9900"/>
                        <text>{{clientBaseData.nationText||'未填写'}}</text>
                    </view>
                    <view>
                        <icon class="iconfont icon-chushengriqi" style="font-size:40rpx;margin-right:20rpx;color:#ff9900"/>
                        <text>{{clientBaseData.birthday||'未填写'}}</text>
                    </view>
                    <view>
                        <icon class="iconfont icon-zhiye" style="font-size:40rpx;margin-right:20rpx;color:#ff9900"/>
                        <text>{{clientBaseData.occupation||'未填写'}}</text>
                    </view>
                    <view>
                        <icon class="iconfont icon-youxiang" style="font-size:40rpx;margin-right:20rpx;color:#ff9900"/>
                        <text>{{clientBaseData.email||'未填写'}}</text>
                    </view>
                </view>
            </view>
            <view>
                <view>
                    <view>
                        <text>企业公司注册编号</text>
                        <text>{{clientBaseData.registeredNo||'未填写'}}</text>
                    </view>
                    <view>
                        <text>企业工商注册名称</text>
                        <text>{{clientBaseData.registeredName||'未填写'}}</text>
                    </view>
                    <view>
                        <text>客户行业</text>
                        <text>{{clientBaseData.industryTypeText||'未填写'}}</text>
                    </view>
                    <view>
                        <text>组织结构</text>
                        <text>{{clientBaseData.organizationUnitText||'未填写'}}</text>
                    </view>
                </view>
            </view>
            <view>
                <view>
                    <view>
                        <text>客户地址</text>
                        <text>{{clientBaseData.address||'未填写'}}</text>
                    </view>
                    <view>
                        <text>邮编号码</text>
                        <text>{{clientBaseData.post||'未填写'}}</text>
                    </view>
                    <view>
                        <text>主页</text>
                        <text>{{clientBaseData.homePage||'未填写'}}</text>
                    </view>
                    <view>
                        <text>客户来源</text>
                        <text>{{clientBaseData.originText||'未填写'}}</text>
                    </view>
                </view>
            </view>
            <view>
                <view>
                    <view>
                        <text>描述</text>
                        <text>{{clientBaseData.description||'未填写'}}</text>
                    </view>
                    <view>
                        <text>备注</text>
                        <text>{{clientBaseData.remark||'未填写'}}</text>
                    </view>
                </view>
            </view>
        </view>
        
    <view  wx:if="{{$placeHolderImage$placeHolder.placeHolderShow}}" class="placeHolderImage" catchtouchmove="$placeHolderImage$true">
                <image src="{{$placeHolderImage$placeHolderImage[$placeHolderImage$placeHolder.placeHolderImageIndex]}}" mode="scaleToFill" lazy-load="false" style="width:580rpx;height:580rpx">
                </image>
    </view>

    </view>