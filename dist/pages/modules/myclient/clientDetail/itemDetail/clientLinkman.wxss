@import "./../../../../../components/placeHolderImage.wxss";
.body {
  height: 100%;
}
.body .fixed {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 1;
  right: 0;
  background: #fff;
}
.body .container-title {
  height: 104rpx;
  font-size: 48rpx;
  padding: 0 28rpx;
  color: #2a2a2a;
  line-height: 104rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #dedede;
}
.body .linkman-container {
  padding: 149rpx 28rpx 0 28rpx;
  font-size: 28rpx;
}
.body .linkman-container .linkman-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.body .linkman-container .linkman-item .linkman-item-left {
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
}
.body .linkman-container .linkman-item .linkman-item-right {
  width: 80%;
  border-bottom: 1px solid #f4f4f4;
  padding: 30rpx 0;
}
.body .linkman-container .linkman-item .linkman-item-right > text:first-child {
  padding-right: 18rpx;
  border-right: 1px solid #2a2a2a;
}
.body .linkman-container .linkman-item .linkman-item-right > text:nth-child(2) {
  padding-left: 20rpx;
  color: #7a7a7a;
}
.body .linkman-container .linkman-item .linkman-item-right > view {
  color: #7a7a7a;
}
