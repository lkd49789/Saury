
    <!-- <view></view> -->
    <!-- <text></text> -->
    <view class="body">
      <view class="linkman-header">
          <!-- <image src="{{avatar}}" mode="aspectFill"><image> -->
            <image class="linkman-avatar" src="{{linker.avatar}}" mode="aspectFill"/>
        <view class="linkman-name">
          <text>{{linker.name}}</text>
          <text>{{linker.dutyText||'未填写'}}</text>
        </view>
        <view class="linkman-mode">
          <view>
            <icon class="iconfont icon-dianhuaxiantiao" style="font-size:60rpx;color:#7a7a7a"/>
          </view>
          <view>
            <icon class="iconfont icon-youxiang2" style="font-size:50rpx;color:#7a7a7a"/>
          </view>
          <view>
            <icon class="iconfont icon-xinxi" style="font-size:50rpx;color:#7a7a7a"/>
          </view>
        </view>
      </view>
        <view class="linkman-info">
          <view class="info-username">
            <text>{{linker.enName||"未填写"}}</text>
            <icon class="iconfont icon-nv" style="color:#ff4367;font-size:34rpx"  wx:if="{{linker.sexText=='女'}}"/>
            <icon class="iconfont icon-nan" style="color:#009dff;font-size:34rpx"  wx:else="{{linker.setText=='男'}}"/>
          </view>
          <view>
            <icon class="iconfont icon-dianhua icon"/>
            <text>{{linker.workPhone||linker.workPhone2||linker.personPhone||linker.personPhone2||'未填写'}}</text>
          </view>
          <view>
            <icon class="iconfont icon-dianhuazuoji1 icon"/>
            <text>{{linker.landline||'未填写'}}</text>
          </view>
          <view>
            <icon class="iconfont icon-chuanzhen1 icon"/>
            <text>{{linker.fax||'未填写'}}</text>
          </view>
          <view>
            <icon class="iconfont icon-youxiang1 icon"/>
            <text>{{linker.workEmail||linker.perEmail||'未填写'}}</text>
          </view>
          <view>
            <icon class="iconfont icon-gongsi icon"/>
            <text>{{linker.company}}</text>
          </view>
          <view>
            <icon class="iconfont icon-dizhi1 icon"/>
            <text>{{linker.address||'未填写'}}</text>
          </view>
        </view>
    </view>