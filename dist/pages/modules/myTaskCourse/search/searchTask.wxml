    <view class="picker-date">
        <view>{{$creationTime$pickerData.title}}</view>
        <view style="display:flex;align-items:center;">
            <picker id="startDate" mode="date" value="{{$creationTime$pickerData.startDateData}}" start="2000-01-01" end="2050-12-31" bindchange="$creationTime$bindDateChange" style="flex:1">
                <view class="tui-picker-detail">
                    {{$creationTime$pickerData.startDateData||'请选择'}}
                    <icon class="updown iconfont icon-gengduoneirongdianjichakanxiangqing" style="font-size:30rpx;color:#b2b2b2"/>
                </view>
            </picker>
            <text style="margin:0 20rpx">至</text>
            <picker id="endDate" mode="date" value="{{$creationTime$pickerData.endDateData}}" start="2000-01-01" end="2050-12-31" bindchange="$creationTime$bindDateChange" style="flex:1">
