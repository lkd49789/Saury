
    <!-- <view></view> -->
    <!-- <text></text> -->
    <view class="body">
        <view class="container-title fixed">
            <text class="title-text">日志文档</text>
        </view>
        <view class="doc-item-contianer">
            <block wx:for="{{logAttachmentList}}" wx:for-index="index" wx:for-item="item" wx:key="index">
                <view class="doc-item" bindtap="preview"  wx:hidden="{{showVideo}}" data-wpypreview-a="{{item.id}}" data-wpypreview-b="{{item.extension}}" data-wpypreview-c="{{index}}" data-wpypreview-d="{{item.size}}">
                    <icon class="iconfont {{fileIcon[index]}}" style="font-size:80rpx;color:{{fileColor[index]}}"/>
                    <view class="doc-left-content">
                        <text class="texthide">{{item.name}}</text>
                        <view style="height:20rpx;width:{{progress[index]}}%;background:#7a7a7a;margin-top:10rpx"/>
                    </view>
                    <!-- <view> -->
                </view>
            </block>
            <view id="videoView" class="video-view"  wx:if="{{showVideo}}"  capture-catch:tap="colseVideo" catchtouchmove="true">
                <view class="videoContent">
                    <video id="myVideo" style="width:100%;" src="{{videoPath}}" danmu-list="{{danmuList}}" objectFit="cover" enable-danmu danmu-btn page-gesture="{{true}}" controls/>
                </view>
            </view>
        </view>
    </view>