.container {
  margin: 44rpx 28rpx;
}
.container .keyword-search .search_top {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.container .keyword-search .search_top .search_input {
  width: 100%;
  padding: 10rpx 20rpx;
  border-radius: 100rpx;
  background: #f4f4f4;
  display: flex;
  align-items: center;
  margin-right: 14rpx;
}
.container .keyword-search .search_top .search_input > icon {
  margin-right: 14rpx;
}
.container .keyword-search .search_top .search_input > input {
  width: 100%;
}
.container .keyword-search .search_top .top_right_btn {
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .keyword-search .search_history {
  margin-top: 28rpx;
}
.container .keyword-search .search_history .history_title {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.container .keyword-search .search_history .history_content {
  margin-top: 10rpx;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.container .keyword-search .search_history .history_content > view {
  background: #f4f4f4;
  padding: 10rpx 20rpx;
  border-radius: 50rpx;
  color: #7a7a7a;
  margin: 14rpx;
  position: relative;
}
.container .keyword-search .search_history .history_content > view > icon {
  position: absolute;
  top: -30rpx;
  right: -10rpx;
}
