
    <view class="container">
        <view class="keyword-search"  wx:if="{{showPage}}">
            <view class="search_top">
                <view class="search_input">
                    <icon class="iconfont icon-sousuo1 input_sousuo" style="color:#5d73fa;font-size:24px;"/>
                    <input value="{{searchKeyWords}}" type="text" confirm-type="search" placeholder="案件名称、客户名称" placeholder-class="placeholder" maxlength="30" bindconfirm="submitSearch" focus="true">
                    </input>
                </view>
                <view class="top_right_btn">
                    <icon class="iconfont icon-gaojisousuo" style="color:#5d73fa;font-size:35px;" bindtap="showPage"/>
                </view>
            </view>
            <view class="search_history">
                <view class="history_title">
                    <text>历史搜索</text>
                    <icon class="iconfont icon-shanchu2" style="color:#7a7a7a;font-size:35rpx;" catchtap="deletItemAll" wx:if="{{History_KeyWord.length!==0}}"/>
                </view>
                <view class="history_content">
                    <block wx:for="{{History_KeyWord}}" wx:for-index="index" wx:for-item="item" wx:key="index">
                        <view bindtap="history" bindlongpress="longTap" data-wpyhistory-a="{{item}}" data-wpylongtap-a="{{index}}">
                            <text>{{item}}</text>
                            <icon class="iconfont icon-zirenwushanchu" style="color:#dfdfdf;font-size:30rpx;" wx:if="{{isShowArray[index]}}" catchtap="deletItem" data-wpydeletitem-a="{{index}}"/>
                        </view>
                    </block>
                </view>
            </view>
        </view>
        <view class="advanced-search-input-panel"  wx:else="{{showPage}}">
            <view class="advanced-search-title">
                <text>高级搜索</text>
                <icon class="iconfont icon-zirenwushanchu" style="color:#7a7a7a;font-size:40rpx;" bindtap="showPage"/>
            </view>
            <view class="advanced-search-content">
                
<view>
     <view class="textarea"  wx:if="{{$Id$isShow}}">
        <view>
            <text>{{$Id$input.title}}</text>
        </view>
        <view class="textarea-container" style="border-bottom:1px solid {{$Id$inputChecked?'#5d73fa':'#dedede'}};color:#7a7a7a">
            <textarea name="{{$Id$input.name}}" bindfocus="$Id$bindfocus" bindblur="$Id$bindblur" placeholder="请输入{{$Id$input.title}}" bindinput="$Id$bindinput" maxlength="-1" value="{{$Id$inputValue}}" auto-height cursor-spacing="5px" fixed="{{$Id$true}}" style="width:83%"/>
        </view>
        <view wx:if="{{$Id$input.warning}}" style="color:#e20000">
                <text>此字段是必填字段</text>
        </view>
    </view>
    <view class="input"  wx:else>
        <view>
            <text>{{$Id$input.title}}</text>
        </view>
        <view class="input-container" style="border-bottom:1px solid {{$Id$inputChecked?'#5d73fa':'#dedede'}};color:#7a7a7a">
            <input name="{{$Id$input.name}}" bindfocus="$Id$bindfocus" bindblur="$Id$bindblur" placeholder="请输入{{$Id$input.title}}" bindinput="$Id$bindinput" disabled="{{$Id$input.disabled}}" value="{{$Id$inputValue}}" type="{{$Id$input.type||'text'}}" confirm-type="done" cursor-spacing="5px"/>
        </view>
        <view wx:if="{{$Id$input.warning}}" style="color:#e20000">
                <text>此字段是必填字段</text>
        </view>
    </view>
   
</view>
    

                
