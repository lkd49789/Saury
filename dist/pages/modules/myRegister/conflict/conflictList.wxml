
    <view class="container">
        <view class="pageTitle">
            <text>利益冲突列表</text>
        </view>
        <view class="list-box">
            <block wx:for="{{conflictListData}}" wx:for-index="index" wx:for-item="item" wx:key="index">
                <view bindtap="toConflictDetial" data-wpytoconflictdetial-a="{{index}}">
                    <view class="name">
                        <text>{{item.name}}</text>
                        <icon class="iconfont icon-gengduoneirongdianjichakanxiangqing" style="font-size:40rpx;color:#bfbfbf" catchtap="compileBtn" data-wpycompilebtn-a="{{index}}"/>
                    </view>
                    <view class="category">
                        {{item.category}}
                    </view>
                    <view class="legalType">
                        <icon class="iconfont icon-ren" style="font-size:40rpx;color:#ff9900"/>
                        <text>{{item.linker||'未填写'}}</text>
                        <text style="color:#7a7a7a">{{item.legalTypeText||'未填写'}}</text>
                    </view>
                    <view class="phone">
                        <icon class="iconfont icon-dianhua1" style="font-size:40rpx;color:#ff9900;margin-right:20rpx"/>
                        <text>{{item.phone||'未填写'}}</text>
                    </view>
                </view>
            </block>
        </view>
        <image src="{{'../../../../images/add.png'}}" mode="scaleToFill" lazy-load="true" class="addImage" bindtap="toCreateConflict">
        </image>
    </view>