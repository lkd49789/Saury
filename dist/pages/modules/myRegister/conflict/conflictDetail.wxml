
    <view class="container">
        <view class="pageTitle">
            <view>
                <text>利益冲突详情</text>
            </view>
            <view>
                <icon class="iconfont icon-bianji" style="font-size:60rpx;color:#5d73fa" bindtap="toAuditPage"/>
            </view>
        </view>
        <view class="firstPart">
            <view>
                 <text>类别</text>
                <text>{{conflictInfo.category||'未填写'}}</text>
            </view>
            <view>
                <text>名称（中）</text>
                <text>{{conflictInfo.name||'未填写'}}</text>
            </view>
            <view>
                <text>名称（英）</text>
                <text>{{conflictInfo.enName||'未填写'}}</text>
            </view>
        </view>
        <view style="width:100%;height:20rpx;background:#f4f4f4"/>
        <view class="twoPart">
            <view class="flex-x">
                <view>
                    <text>联系人</text>
                    <text>{{conflictInfo.linker||'未填写'}}</text>
                </view>
                <view>
                    <text>职务</text>
                    <text>{{conflictInfo.legalTypeText||'未填写'}}</text>
                </view>
            </view>
            <view class="flex-x">
                <view>
                    <text>电话</text>
                    <text>{{conflictInfo.phone||'未填写'}}</text>
                </view>
                <view>
                    <text>邮箱地址</text>
                    <text>{{conflictInfo.email||'未填写'}}</text>
                </view>
            </view>
            <view class="flex">
                    <text>身份证</text>
                    <text>{{conflictInfo.cardId||'未填写'}}</text>
            </view>
            <view class="flex">
                    <text>地址</text>
                    <text>{{conflictInfo.address||'未填写'}}</text>
            </view>
            <view class="flex">
                    <text>备注</text>
                    <text>{{conflictInfo.remark||'未填写'}}</text>
            </view>
        </view>
    </view>