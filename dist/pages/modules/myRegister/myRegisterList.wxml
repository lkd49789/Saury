
    <view class="container">
        <view class="top-title">
            <text>我的立案</text>
            <icon class="iconfont icon-sousuo1" style="font-size:60rpx;color:#5d73fa" bindtap="toSearch"/>
        </view>
        <view class="main">
            <block wx:for="{{CaseRecordListDatas}}" wx:for-index="index" wx:for-item="item" wx:key="index">
                <view class="regist-item" bindtap="toRegistDetail" data-wpytoregistdetail-a="{{item.id}}" data-wpytoregistdetail-b="{{item.clientId}}">
                    <view>{{item.name}}</view>
                    <view>{{item.clientName||'未填写'}}</view>
                    <view>{{item.categoryText||'未填写'}}</view>
                    <view>
                        <view>
                            <text style="border:1px solid #ff9900"/>
                            <text>{{item.processStatusText}}</text>
                        </view>
                        <text>{{item.acceptDateText}}</text>
                    </view>
                    <icon class="iconfont icon-gengduo" style="font-size:40rpx;color:#b2b2b2" catchtap="operations" data-wpyoperations-a="{{item}}"/>
                </view>
            </block>
        </view>
        <image src="{{'../../../images/add.png'}}" mode="scaleToFill" lazy-load="true" class="addImage" bindtouchend="touchEnd"/>
    </view>