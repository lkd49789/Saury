page {
  position: relative;
}
page .placeHolderImage {
  position: fixed;
  top: 0rpx;
  left: 0;
  height: 100%;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 9999;
  background: #fff;
}
page .Full-screen {
  position: fixed;
  top: 0rpx;
  left: 0;
  height: 100%;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 999;
  background: rgba(0, 0, 0, 0.3);
}
page .Full-screen > view {
  height: 550rpx;
  width: 73%;
  background: #fff;
  position: relative;
}
page .Full-screen > view > view:first-child {
  width: 110rpx;
  height: 110rpx;
  background: #5d73fa;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 80rpx;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 1000;
  color: #fff;
}
page .Full-screen > view .fixed-text-count {
  position: absolute;
  bottom: 100rpx;
  right: 30rpx;
  font-size: 28rpx;
  color: #b2b2b2;
}
page .Full-screen > view > view:last-child {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 75rpx;
}
page .Full-screen > view > view:last-child > view:first-child {
  font-size: 36rpx;
  font-weight: bold;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  padding-bottom: 5rpx;
  border-bottom: 1px solid #dfdfdf;
  padding-bottom: 20rpx;
  margin-bottom: 20rpx;
}
page .Full-screen > view > view:last-child > view:nth-child(2) {
  color: #7a7a7a;
}
page .Full-screen > view > view:last-child > view:last-child {
  display: flex;
  align-items: center;
  flex-direction: row;
  width: 100%;
  height: 80rpx;
  position: absolute;
  bottom: 0;
}
page .Full-screen > view > view:last-child > view:last-child > view {
  width: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #5d73fa;
  color: #fff;
  height: 100%;
}
page .Full-screen > view > view:last-child > view:last-child > view:first-child {
  border-right: 1px solid #fff;
}
page .Full-screen-two {
  position: fixed;
  top: 0rpx;
  left: 0;
  height: 100%;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 999;
  background: rgba(0, 0, 0, 0.3);
}
page .Full-screen-two > view {
  height: 70%;
  width: 80%;
  background: #fff;
  border-radius: 20rpx;
  padding: 44rpx 28rpx;
}
page .Full-screen-two > view > view:first-child {
  font-size: 36rpx;
  margin-bottom: 44rpx;
}
page .Full-screen-two > view > view:nth-child(2) {
  font-size: 32rpx;
  margin-bottom: 30rpx;
  display: flex;
  flex-direction: column;
}
page .Full-screen-two > view > view:nth-child(2) > text:last-child {
  font-weight: bold;
  color: #7a7a7a;
}
page .Full-screen-two > view > view:nth-child(3) {
  border-bottom: 1px solid #dfdfdf;
  padding-bottom: 30rpx;
  margin-bottom: 30rpx;
}
page .Full-screen-two > view > view:nth-child(4) {
  margin-bottom: 30rpx;
  border-bottom: 1px solid #dfdfdf;
  padding-bottom: 30rpx;
}
page .Full-screen-two > view > view:nth-child(4) > view:first-child {
  margin-bottom: 20rpx;
  font-weight: bold;
  color: #7a7a7a;
}
page .Full-screen-two > view > view:nth-child(4) > view:last-child {
  display: flex;
  align-items: center;
}
page .Full-screen-two > view > view:last-child {
  display: flex;
  flex-direction: row-reverse;
}
page .Full-screen-two > view > view:last-child > view {
  padding: 20rpx 30rpx;
  margin-left: 20rpx;
  border-radius: 10rpx;
}
.body {
  height: 100%;
}
.body .main > view:not(:last-child):not(:first-child):after {
  display: block;
  content: '';
  height: 14rpx;
  background: #f4f4f4;
}
.body .main > view:not(:first-child) > view {
  padding: 30rpx 28rpx;
}
.body .main .participant {
  margin-right: 20rpx;
}
.body .main .participant > image {
  height: 60rpx;
  width: 60rpx;
  border-radius: 50%;
}
.body .main .titleChunk {
  border-bottom: 1px solid #dfdfdf;
  padding: 44rpx 28rpx;
  font-size: 34rpx;
}
.body .main .oneChunk > view > view {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.body .main .oneChunk > view > view > view {
  display: flex;
  align-items: center;
}
.body .main .oneChunk > view > view:not(:last-child) {
  margin-bottom: 20rpx;
}
.body .main .twoChunk > view > view {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.body .main .twoChunk > view > view:not(:last-child) {
  margin-bottom: 20rpx;
}
.body .main .recordChunk > view > view:first-child {
  color: #7a7a7a;
  margin-bottom: 30rpx;
}
.body .main .recordChunk > view > view:last-child {
  display: flex;
  align-items: center;
}
.body .main .threeChunk > view > text {
  display: flex;
  flex-direction: column;
}
.body .main .threeChunk > view > text:not(:last-child) {
  margin-bottom: 20rpx;
}
.body .main .threeChunk > view > text:nth-child(2n+1) {
  color: #7a7a7a;
}
.body .main .fourChunk > view > text {
  display: flex;
  flex-direction: column;
}
.body .main .fourChunk > view > text:not(:last-child) {
  margin-bottom: 30rpx;
}
.body .main .fourChunk > view > text:first-child {
  color: #7a7a7a;
}
.body .main .fiveChunk > view > view:first-child {
  margin-bottom: 30rpx;
  color: #7a7a7a;
}
.body .main .fiveChunk > view > view:not(:first-child):not(:last-child) {
  margin-bottom: 20rpx;
}
.body .main .fiveChunk > view .docItem {
  display: flex;
  align-items: center;
}
.body .main .fiveChunk > view .docItem > icon {
  font-size: 60rpx;
  margin-right: 20rpx;
}
.body .main .sixChunk > view > view:first-child {
  color: #7a7a7a;
  margin-bottom: 30rpx;
}
.body .main .sixChunk > view > view:not(:first-child):not(:last-child) {
  margin-bottom: 25rpx;
}
.body .main .sixChunk > view > view {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.body .main .sevenChunk > view > view:first-child {
  margin-bottom: 30rpx;
  color: #7a7a7a;
}
.body .main .sevenChunk > view > view:last-child {
  display: flex;
  align-items: center;
}
.body .main .eightChunk > view > view:nth-child(1) {
  margin-bottom: 30rpx;
  color: #7a7a7a;
}
.body .main .eightChunk > view > view:nth-child(3) {
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #7a7a7a;
}
.body .main .eightChunk > view > view:nth-child(4) {
  display: flex;
}
.body .main .eightChunk > view > view:nth-child(4) > view:first-child {
  width: 25%;
}
.body .main .eightChunk > view > view:nth-child(4) > view:last-child {
  width: 75%;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.body .main .eightChunk > view > view:nth-child(4) > view:last-child > text {
  border: 1px dotted #5d73fa;
  padding: 10rpx 15rpx;
  margin: 0 14rpx 14rpx 0;
}
.body .main .eightChunk > view > view:nth-child(5) {
  display: flex;
}
.body .main .eightChunk > view > view:nth-child(5) > view:first-child {
  width: 25%;
}
.body .main .eightChunk > view > view:nth-child(5) > view:last-child {
  width: 75%;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.body .main .eightChunk > view > view:nth-child(5) > view:last-child > text {
  border: 1px dotted #5d73fa;
  padding: 10rpx 15rpx;
  margin: 0 14rpx 14rpx 0;
}
.body .main .eightChunk > view > view:nth-child(6) {
  display: flex;
}
.body .main .eightChunk > view > view:nth-child(6) > view:first-child {
  width: 25%;
}
.body .main .eightChunk > view > view:nth-child(6) > view:last-child {
  width: 75%;
}
.body .main .eightChunk > view > view:not(:first-child):not(:last-child) {
  margin-bottom: 20rpx;
}
.body .bottom-fixed {
  position: fixed;
  height: 98rpx;
  width: 100%;
  bottom: 0;
  display: flex;
  align-items: center;
  background: #5d73fa;
}
.body .bottom-fixed > view {
  width: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 34rpx;
  color: #fff;
  height: 100%;
}
.body .bottom-fixed > view:first-child {
  border-right: 1px solid #fff;
}
.body .bottom-fixed-record {
  position: fixed;
  height: 98rpx;
  width: 100%;
  bottom: 0;
  border-top: 1px solid #dfdfdf;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 34rpx;
  background: #fff;
  color: #5d73fa;
}
