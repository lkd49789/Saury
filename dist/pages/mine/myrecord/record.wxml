                </view>
                <view class="container-main">
                  <view class="one-line">
                    <view><text style="color:#2a2a2a;font-size:28rpx">{{item.workUnits||'未填写'}}</text></view>
                    <view style="margin-top:20rpx;"><text style="color:#5d74fa;">{{item.jobContent||'未填写'}}</text></view>
                  </view>
                  <view class="two-line">
