
    <view class="body">
      <view class="self-title">
        <text class="self-text">个人简历</text>
      </view>
      
    <view class="navbar">
        <scroll-view scroll-x>
            <block wx:for="{{$navbar$navbars}}" wx:for-index="$navbar$index" wx:for-item="$navbar$item" wx:key="$navbar$index">
                <view class="item {{$navbar$currentTab == $navbar$index ? 'active' : ''}} " bindtap="$navbar$navbarTap" data-wpynavbartap-a="{{$navbar$index}}">
                    <text>{{$navbar$item}}</text>
                    <view  wx:if="{{$navbar$currentTab == $navbar$index}}" class="navbar_box"/>
                </view>
            </block>
        </scroll-view>
    </view>

          <view class="container "  wx:if="{{currentTab==0}}">
            <block wx:for="{{educationsData}}" wx:for-index="index" wx:for-item="item" wx:key="index">
                <view class="bg-container">
                  <view class="container-top">
                    <view class="icon">
                      <text class="iconfont icon-shijian" style="color:#ffcb46;font-size:40rpx ;margin-right:30rpx"/>
                    </view>
                    <view class="content">
                      <text>{{item.startDate||'未填写'}} 至 {{item.endDate||'未填写'}}</text>
                      <!-- <text class="iconfont icon-gengduoneirongdianjichakanxiangqing" style="color:#7a7a7a;font-size:30rpx;" bindtap="chooseSezi"></text> -->
                    </view>
                  </view>
                  <view class="container-main">
                    <view class="one-line">
                      <text style="color:#2a2a2a;font-size:28rpx">{{item.school||'未填写'}}</text>
                      <view class="education-degree">
                        <text>{{item.education||'未填写'}}</text>
                        <text>{{item.degree||'未填写'}}</text>
                      </view>
                    </view>
                    <view class="two-line">
                      <text>{{item.speciality||'未填写'}}</text>
                    </view>
                    <view class="three-line">
                      <text>{{item.memo||'未填写'}}</text>
                    </view>
                  </view>
                  <!-- 固定定位修改图标 -->
                  <!-- 向上弹出框 -->
                </view>
            </block>
           
          </view>
          <view class="container "  wx:if="{{currentTab == 1}}">
