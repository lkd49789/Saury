
  <view class="body">
    <view class="swiper-view"  wx:if="{{GetPersonAnnualCounts.length}}">
      <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{circular}}" next-margin="40rpx" previous-margin="40rpx" bindchange="bindchange" style="height:405rpx">
        <block wx:for="{{GetPersonAnnualCounts}}" wx:for-index="index" wx:for-item="item" wx:key="index">
          <swiper-item>
            <view class="swiper-item" bindtap="toSwiper">
              <!-- <view class="flex">
                <view class="swperBox">
                  <text>{{item.name}}</text>
                  <text>{{item.value}}</text>
                </view>
                  <view class="swiperCompare">
                    
                  </view>
              </view> -->
              <view>
                <text>{{item.value}}</text>
              </view>
              <view>
                <text>{{item.name}}</text>
              </view>
              <view style="color:{{item.comparedValue>0?'#ff9900':'#069400'}}">
                <icon class="iconfont {{item.comparedValue>0?'icon-jiantoushangshengtongji':'icon-jiantouxiajiangtongji'}}"/>
                <text>{{comparedValue[index]||item.comparedValue}}%</text>
              </view>
              <view>
                <view>
                  <text style="color:#7a7a7a">去年 |</text>
                  <text> {{item.annualData[1].value}}</text>
                </view>
                <view>
                  <text style="color:#7a7a7a">今年 |</text>
                  <text> {{item.annualData[0].value}}</text>
                </view>
              </view>
            </view>
          </swiper-item>
        </block>
      </swiper>
    </view>
    <view class="function">
      <view class="auditFuntion">
        <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" next-margin="50rpx" style="height:200rpx" display-multiple-items="3">
          <block wx:for="{{auditFunction}}" wx:for-index="index" wx:for-item="item" wx:key="index">
            <swiper-item>
              <view class="swiper-item" bindtap="linkAddress" data-wpylinkaddress-a="{{item.link}}">
                <image src="{{item.image}}" mode="scaleToFill" lazy-load="false"/>
                <view>
                  <icon class="iconfont {{item.icon}}" style="font-size:35px;"/>
                  <text>{{item.displayName}}</text>
                </view>
              </view>
            </swiper-item>
          </block>
        </swiper>
      </view>
      <view class="Notifications" bindtap="toMessagePage">
        <view class="tishi"  wx:if="{{GetUserNotifications_data.length!==0}}">
          <icon class="iconfont icon-tishi" style="font-size:70rpx;color:#ff9900"/>
        </view>
        <swiper autoplay interval="3000" circular="circular" vertical="{{true}}" skip-hidden-item-layout="{{true}}" display-multiple-items="{{GetUserNotifications_data.length==1?1:2}}" style="height:{{GetUserNotifications_data.length==1?41:82}}rpx;width:86%;" wx:if="{{GetUserNotifications_data.length!==0}}">
          <block wx:for="{{GetUserNotifications_data}}" wx:for-index="index" wx:for-item="item" wx:key="index">
            <swiper-item catchtouchmove="true">
              <view class="remind_swiper">
                <view>
                  <view/>
                  <text>{{item.TextTile}}</text>
                </view>
                <text style="color:#7a7a7a">{{item.notification.creationTime}}</text>
              </view>
            </swiper-item>
          </block>
        </swiper>
        <view class="notData" wx:else="{{GetUserNotifications_data.length!==0}}">
          <!-- <icon class="iconfont icon-tishi" style="font-size:70rpx;color:#ff9900"></icon> -->
          <image src="../../images/NotMessage.png" mode="scaleToFill" lazy-load="false" style="height:60rpx;width:60rpx;margin-right:20rpx">
          </image>
          <text>暂无新消息！</text>
        </view>
      </view>
      <view class="Function-items">
        <block wx:for="{{functionData}}" wx:for-index="index" wx:for-item="item" wx:key="index">
          <view bindtap="linkAddress" data-wpylinkaddress-a="{{item.link}}">
            <view>
              <icon class="iconfont {{item.icon}}" style="color:{{item.color}};font-size:48rpx;"/>
            </view>
            <text>{{item.displayName}}</text>
          </view>
        </block>
        <view bindtap="linkAddress" data-wpylinkaddress-a="../modules/zzbangVip/getCasePageList">
            <view>
              <icon class="iconfont icon-zhaizhubang" style="color:#e20000;font-size:48rpx;"/>
            </view>
            <text>案源</text>
        </view>
      </view>
      <!-- <view bindtap="click">点击跳转</view> -->
    </view>
  </view>